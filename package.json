{
  "name": "ddk",
  "version": "0.9.9a",
  "main": "index.js",
  "private": true,
  "scripts": {
    "tsc": "tsc",
    "build": "npm run build:core & npm run build:api",
    "start": "npm run start:core & npm run start:api",
    "start:api": "node --max-old-space-size=8192 dist/api/app.js",
    "start:core": "node --max-old-space-size=8192 dist/core/app.js",
    "server": "npm run server:core & npm run server:api",
    "server:api": "nodemon --max-old-space-size=8192 --watch dist/api/app.js dist/api/app.js",
    "server:core": "nodemon --max-old-space-size=8192 --watch dist/core/app.js dist/core/app.js",
    "debug": "node --inspect=0.0.0.0:9229 app.js",
    "jsdoc": "jsdoc -c docs/conf.json --verbose --pedantic",
    "server-docs": "npm run jsdoc && http-server docs/jsdoc/",
    "build:api": "./node_modules/webpack/bin/webpack.js --env.service=api --progress --profile --colors --display-modules",
    "build:core": "./node_modules/webpack/bin/webpack.js --env.service=core --progress --profile --colors --display-modules",
    "build:genesis": "./node_modules/webpack/bin/webpack.js --env.service=genesis",
    "generate": "npm run build:genesis && NODE_ENV_IN=development node --max-old-space-size=8192 dist/generateGenesis.js",
    "watch": "npm run watch:core & npm run watch:api",
    "watch:api": "./node_modules/webpack/bin/webpack.js --env.service=api --progress --colors --watch",
    "watch:core": "./node_modules/webpack/bin/webpack.js --env.service=core --progress --colors --watch",
    "clean": "rm -rf node_modules/ dist/ dapps/ .nyc_output/ logs/ .npm/ .config/ .node-gyp",
    "watch:with_update": "npm install && npm run watch",
    "watch:with_update_clean": "npm run clean && npm install && npm run watch",
    "test": "export NODE_PATH=./ && mocha --require ts-node/register './test/**/*.test.ts'",
    "test:decorator": "export NODE_PATH=./ && mocha --require ts-node/register './test/shared/decorator/*.test.ts'",
    "test:migrate": "export NODE_PATH=./ && mocha --require ts-node/register './test/shared/migrate/*.test.ts'",
    "test:address": "export NODE_PATH=./ && mocha --require ts-node/register './test/shared/address/*.test.ts'",
    "test:mock": "export NODE_PATH=./ && mocha --require ts-node/register './test/core/mock/*.test.ts'",
    "test:roundservice": "export NODE_PATH=./ && mocha --require ts-node/register './test/core/service/*.test.ts'",
    "test:roundrepository": "export NODE_PATH=./ && mocha --require ts-node/register './test/core/repository/round.test.ts'",
    "test:decorator:rpc": "export NODE_PATH=./ && mocha --require ts-node/register './test/api/util/rpc.test.ts'"
  },
  "author": "DDK Foundation <http://www.ddkoin.com/>",
