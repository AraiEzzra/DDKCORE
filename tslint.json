{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [ false, "single" ],
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "eofline": false,
        "variable-name": false,
        "no-string-literal": false,
        "prefer-for-of": false,
        "max-line-length": [ true, 170 ],
        "no-angle-bracket-type-assertion": false,
        "no-console": false,
        "no-bitwise": false,
        "no-trailing-whitespace": false,
        "member-access": ["no-public"],
        "no-namespace": false
    },
    "rulesDirectory": []
}